{
  "type": "app",
  "name": "AMMP Config",
  "id": "ammp-app",

  "css": {
    "dark":  "css/dark.css",
    "light":  "css/light.css"
  },

  "info": {
    "description": "AMMP config app for Grafana",
    "author": {
      "name": "AMMP",
      "url": "http://www.ammp.io"
    },
    "keywords": ["AMMP app"],
    "logos": {
      "small": "img/small_logo.png",
      "large": "img/large_logo.png"
    },
    "links": [
      {"name": "Project site", "url": "https://github.com/ammpio/grafana-ammp-app"},
      {"name": "License & Terms", "url": "https://github.com/ammpio/grafana-ammp-app/blob/master/LICENSE"}
    ],
    "version": "1.0.2",
    "updated": "2015-06-06"
  },
  

  "includes": [
    {"type": "page", "name": "Assets", "component": "AssetPageCtrl", "role": "Viewer", "addToNav": true,  "defaultNav": true},
    {"type": "page", "name": "Credentials", "component": "CredentialPageCtrl", "role": "Viewer", "addToNav": true,  "defaultNav": true},
    {"type": "dashboard", "name": "Example App Dashboard"},
    {"type": "panel", "name": "Example App Panel"},
    {"type": "datasource", "name": "Example App Datasource"}
